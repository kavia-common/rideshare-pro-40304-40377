# PUBLIC_INTERFACE
# Example environment variables for the React frontend.
# Ensure these match your backend server and that BACKEND FRONTEND_URL equals REACT_APP_FRONTEND_URL.

REACT_APP_API_BASE=http://localhost:4000
REACT_APP_BACKEND_URL=http://localhost:4000
REACT_APP_WS_URL=ws://localhost:4000/ws
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional toggles
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=0
